import * as assert from 'assert';

console.log('Example:')
console.log(symmetric([
    [0, 1, 2],
    [-1, 0, 1],
    [-2, -1, 0]]))

// These "asserts" using only for self-checking and not necessary for auto-testing

assert.equal(symmetric([
    [0, 1, 2],
    [-1, 0, 1],
    [-2, -1, 0]]), true);
assert.equal(symmetric([
    [0, 1, 2],
    [-1, 1, 1],
    [-2, -1, 0]]), false);
assert.equal(symmetric([
    [0, 1, 2],
    [-1, 0, 1],
    [-3, -1, 0]]), false);
